            steps {
                echo 'Build'
                sleep 5
            }
        }
        stage('Test') {
            steps {
                echo 'Test'
                sleep 5
           }
        }
        stage('Deloy') {
            steps {
                echo 'Deploy'
                sleep 5
            }
        }
    }
        
}
